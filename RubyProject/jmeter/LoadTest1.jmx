        <JSR223Sampler guiclass="TestBeanGUI" testclass="JSR223Sampler" testname="Load test 1" enabled="true">
          <stringProp name="scriptLanguage">groovy</stringProp>
          <stringProp name="parameters"></stringProp>
          <stringProp name="filename"></stringProp>
          <stringProp name="cacheKey">true</stringProp>
          <stringProp name="script">server = ${server};
port = ${port};
def socket = new Socket(server, port);
r = new BufferedReader(new InputStreamReader(socket.getInputStream()));
w = new BufferedWriter(new OutputStreamWriter(socket.getOutputStream()));
key = &quot;key&quot;+${__threadNum}
instruction = &quot;add &quot;+key+ &quot; 21312313 1000 5&quot;;

w.write(instruction+&quot;\r\n&quot;);
w.flush();
sleep(500)
w.write(&quot;value\r\n&quot;);
w.flush();
message = r.readLine();
if (message != &quot;STORED&quot;) {
	throw new Exception(&quot;Wrong response&quot;)
}
socket = new Socket(server, port);
r = new BufferedReader(new InputStreamReader(socket.getInputStream()));
w = new BufferedWriter(new OutputStreamWriter(socket.getOutputStream()));
sleep(500);
instruction = &quot;get &quot;+key;
w.write(instruction+&quot;\r\n&quot;);
w.flush();
message = r.readLine();
if (message != &quot;VALUE &quot;+key+&quot; 21312313 5&quot;) {
	throw new Exception(&quot;Wrong response&quot;)
}
sleep(2000)
w.close()
</stringProp>
        </JSR223Sampler>
